{
  "name": "kuaukutsu/yii2-cache",
  "description": "Cache Decorator",
  "keywords": [
    "cache",
    "yii2"
  ],
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Dmitriy Krivopalov",
      "email": "dimarik82@gmail.com"
    }
  ],
  "support": {
    "issues": "https://github.com/kuaukutsu/cache/issues",
    "source": "https://github.com/kuaukutsu/cache"
  },
  "require": {
    "php": ">=7.4",
    "yiisoft/yii2": "^2.0.35"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.4",
    "vimeo/psalm": "^4.1",
    "psalm/plugin-phpunit": "^0.13.0",
    "squizlabs/php_codesniffer": "*",
    "roave/infection-static-analysis-plugin": "^1.3",
    "roave/security-advisories": "dev-latest"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "autoload": {
    "psr-4": {
      "kuaukutsu\\cache\\": "src",
      "kuaukutsu\\cache\\tests\\": "tests"
    }
  }
